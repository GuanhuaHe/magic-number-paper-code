    "ddd = df[(df.LSIM==8) & (abs(df.beta-8.8)<0.001)].set_index(['concentration', 'beta']).drop(['LSIM', 'mean','NSIM'], axis=1).reset_index()\n",
    "\n",
    "aaa = ddd.drop('beta', axis=1).values\n",
    "\n",
    "popt,_ = curve_fit(double_gaussian,aaa[:,0],aaa[:,1])\n",
    "c1, mu1, sigma1, c2, mu2, sigma2 = popt\n",
    "\n",
    "popt,_ = curve_fit(gaus,aaa[:,0],aaa[:,1])\n",
    "c, mu, sigma = popt\n",
    "\n",
    "pylab.plot(aaa[:,0], aaa[:,1], '*')\n",
    "pylab.plot(aaa[:,0], gaus(aaa[:,0], c, mu, sigma))\n",
    "pylab.plot(aaa[:,0], gaus(aaa[:,0], c1, mu1, sigma1))\n",
    "pylab.plot(aaa[:,0], gaus(aaa[:,0], c2, mu2, sigma2))\n",
    "pylab.xlabel('concentration')\n",
    "pylab.ylabel('size fluctuation')"
