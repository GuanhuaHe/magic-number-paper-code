    "    return a*np.exp(-(x-x0)**2.0/(2.0*sigma**2.0))\n",
    "\n",
    "def double_gaussian( x, c1, mu1, sigma1, c2, mu2, sigma2):\n",
    "    res =   c1 * np.exp( - (x - mu1)**2.0 / (2.0 * sigma1**2.0) ) \\\n",
    "          + c2 * np.exp( - (x - mu2)**2.0 / (2.0 * sigma2**2.0) )\n",
    "    return res"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def gaussian_fit(L):\n",
    "    ddd = df[(df.LSIM==L) & ((df.beta*100+1.)%40<1.001)].set_index(['concentration', 'beta']).drop(['LSIM', 'mean','NSIM'], axis=1).reset_index()\n",
    "    Ts = []\n",
    "    x0s = []\n",
    "    for T in np.unique(ddd.beta.values):\n",
    "        aaa = ddd[ddd.beta==T].drop('beta', axis=1).values\n",
    "        popt,_ = curve_fit(gaus,aaa[:,0],aaa[:,1])\n",
    "        a, x0, sigma = popt\n",
    "        Ts.append(T)\n",
    "        x0s.append(x0)\n",
    "    return x0s, Ts"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def double_gaussian_fit(L):\n",
    "    ddd = df[(df.LSIM==L) & ((df.beta*100+1.)%40<1.001)].set_index(['concentration', 'beta']).drop(['LSIM', 'mean','NSIM'], axis=1).reset_index()\n",
    "    Ts = []\n",
    "    x0s = []\n",
    "    for T in np.unique(ddd.beta.values):\n",
    "        try:\n",
    "            aaa = ddd[ddd.beta==T].drop('beta', axis=1).values\n",
    "            popt,_ = curve_fit(double_gaussian,aaa[:,0],aaa[:,1])\n",
    "            c1, mu1, sigma1, c2, mu2, sigma2 = popt\n",
    "            Ts.append(T)\n",
    "            x0 = (c1*mu1 + c2*mu2)/(c1+c2)\n",
    "            x0s.append(x0)\n",
    "        except:\n",
    "            pass\n",
    "    return x0s, Ts"
   ]
  },
